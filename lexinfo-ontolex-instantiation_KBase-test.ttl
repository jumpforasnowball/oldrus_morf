@prefix ontolex: <http://www.w3.org/ns/lemon/ontolex#> .
@prefix lexinfo: <http://lexinfo.net/ontology/2.0/lexinfo#> .

<ОБРАЗ> a ontolex:Word ;
  ontolex:canonicalForm <ОБРАЗ#lemma> ;
  ontolex:otherForm <ОБРАЗ#plural-nominative> ;
  ontolex:otherForm <ОБРАЗ#singular-instrumental> ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lexinfo:gender lexinfo:masculine .

<ОБРАЗ#lemma> 
  ontolex:writtenRep "ОБРАЗ"@ru ;
  ontolex:writtenRep "WБРАЗ"@ru ;
  ontolex:writtenRep "ОБРАЗЪ"@ru ;
  lexinfo:case lexinfo:nominativeCaseForm ;
  lexinfo:gender lexinfo:masculine ;
  lexinfo:number lexinfo:singular .

<ОБРАЗИ#plural-nominative>
  ontolex:writtenRep "ОБРАЗ"@ru ;
  lexinfo:number lexinfo:plural ;
  lexinfo:case lexinfo:nominativeCaseForm .
  
<ОБРАЗОМ#singular-instrumental>
  ontolex:writtenRep "ОБРАЗОМЪ"@ru ;
  lexinfo:number lexinfo:plural ;
  lexinfo:case lexinfo:instrumentalCaseForm .


